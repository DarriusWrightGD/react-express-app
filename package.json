    "express": "^4.13.4",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "nodemon": "^1.9.1",
    "react": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-router": "^2.0.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
