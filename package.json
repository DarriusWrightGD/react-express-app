{
  "name": "react-express-app",
  "version": "1.0.0",
  "description": "React Express App",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "gulp dev",
    "clean": "rm -rf lib",
    "watch-js": "./node_modules/.bin/babel src -d lib -w",
    "dev-server": "node lib/server/webpack",
    "server": "nodemon lib/server/main.js",
    "start": "npm run watch-js & npm run dev-server & npm run server",
    "build": "npm run clean && ./node_modules/.bin/babel src -d lib"
  },
  "author": "DarriusWright",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.7.0",
    "babel-plugin-syntax-async-functions": "^6.5.0",
    "babel-plugin-transform-regenerator": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-preset-stage-3": "^6.5.0",
    "babel-regenerator-runtime": "^6.5.0",
    "babelify": "^7.2.0",
    "body-parser": "^1.15.0",
    "browser-sync": "^2.11.1",
    "browserify": "^13.0.0",
    "ejs": "^2.4.1",
    "express": "^4.13.4",
    "gulp": "^3.9.1",
    "gulp-live-server": "0.0.29",
    "isomorphic-fetch": "^2.2.1",
    "jquery": "^2.2.1",
    "lodash": "^4.6.1",
    "mongoose": "^4.4.7",
    "react": "^0.14.7",
    "react-addons": "^0.9.1-deprecated",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.0.1",
    "require": "^2.4.20",
    "vinyl-source-stream": "^1.1.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0",
      "stage-1",
      "stage-2",
      "stage-3"
    ],
    "plugins": [
      "syntax-async-functions",
      "transform-regenerator"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-loader": "^6.2.4",
    "css-loader": "^0.23.1",
